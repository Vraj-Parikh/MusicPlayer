{
  "expo": {
    "name": "MusicPlayer",
    "slug": "MusicPlayer",
    "scheme": "MusicPlayer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/NewSlashScreenResized.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vraj151.MusicPlayer"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/NewSlashScreenResized.png",
        "backgroundColor": "#000"
      },
      "package": "com.vraj151.MusicPlayer",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-media-library",
        {
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "87632f4f-0af7-43c1-84e1-ee8f35a31bd4"
      }
    }
  }
}
